{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "indent": "off",
        "class-methods-use-this": "off",
        "object-shorthand": "off",
        "no-underscore-dangle": "off",
        "max-len":"off",
        "lines-between-class-members":"off",
        "no-self-assign":"off"
    }
}
